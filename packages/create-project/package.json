{
    "name": "@dyb-dev/create-project",
    "version": "0.1.1",
    "description": "构建项目的脚手架工具",
    "author": "dyb-dev",
    "license": "MIT",
    "type": "module",
    "private": false,
    "bin": {
        "create-project": "./dist/index.cjs"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
