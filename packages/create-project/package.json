    },
    "keywords": [
        "project",
        "project-template",
        "template-project",
        "create-project",
        "create-project-cli"
    ],
    "devDependencies": {
        "@types/node": "^22.7.7",
        "@types/prompts": "^2.4.9",
        "esbuild": "^0.24.0",
        "gradient-string": "^3.0.0",
        "kolorist": "^1.8.0",
        "ora": "^8.1.0",
        "prompts": "^2.4.2"
